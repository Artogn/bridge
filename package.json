{
  "name": "metadisk-api",
  "version": "0.0.0",
  "description": "access the storj network via simple rest api",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "bin": {
    "metadisk": "bin/metadisk.js"
  },
  "scripts": {
    "testsuite": "mocha test/**",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive",
    "linter": "./node_modules/.bin/jshint --config .jshintrc ./index.js ./lib ./test",
    "test": "npm run testsuite && npm run linter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Storj/metadisk-api.git"
  },
  "keywords": [
    "storj",
    "metadisk"
  ],
  "author": "Gordon Hall <gordon@gordonwritescode.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/Storj/metadisk-api/issues"
  },
  "homepage": "https://github.com/Storj/metadisk-api#readme",
  "devDependencies": {
    "chai": "^3.4.1",
    "coveralls": "^2.11.6",
    "istanbul": "^0.4.1",
    "jshint": "^2.8.0",
    "mocha": "^2.3.4",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "express": "^4.13.3",
    "merge": "^1.2.0",
    "mongoose": "^4.3.4"
  }
}
